@model Photo_Video_Studio.Models.MainSlider

@{
    ViewBag.Title = "Details";
}

<h2>Details</h2>

<div>
    <h4>MainSlider</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.Image)
        </dt>

        <dd style="width:50%;">
            <img src="~/Public/images/mainslider/@Model.Image" alt="Alternate Text" />
        </dd>
        <dt>
            Company Name
        </dt>

        <dd>
            @Html.DisplayFor(model => model.RotateHeader)
        </dd>

        <dt>
            Title
        </dt>

        <dd>
            @Html.DisplayFor(model => model.MainTitle)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.Subtitle)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Subtitle)
        </dd>



    </dl>
</div>
<p>
    <a class="btn btn-primary" href="@Url.Action("Index","MainSlider")">Back to List</a>
</p>
